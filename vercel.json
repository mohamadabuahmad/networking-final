{
    "version": 2,
    "builds": [
      {
        "src": "backend/server/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/package.json",
        "use": "@vercel/next" 
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/server/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/$1"
      }
    ]
  }
  